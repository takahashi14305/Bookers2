<%= render partial: 'books/list', locals: { user: @user } %>
<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="1"></th>
    </tr>
  </thead>
  <tbody>
<% @users.each do |user| %>
 <tr>
  <td><%= image_tag user.get_profile_image(100, 100) %></td>
  <td><%= user.name %></td>
  <td><%= link_to "Show", user_path(user) %></td>
 </tr>

<% end %>
  </tbody>
</table>